{
  "dashboard": {
    "id": null,
    "uid": "minio-performance",
    "title": "MinIO Enterprise - Performance Dashboard",
    "tags": ["minio", "performance", "enterprise"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "type": "graph",
        "title": "Operations Throughput (ops/sec)",
        "targets": [
          {
            "expr": "rate(minio_put_ops_total[1m])",
            "legendFormat": "PUT Operations",
            "refId": "A"
          },
          {
            "expr": "rate(minio_get_ops_total[1m])",
            "legendFormat": "GET Operations",
            "refId": "B"
          },
          {
            "expr": "rate(minio_delete_ops_total[1m])",
            "legendFormat": "DELETE Operations",
            "refId": "C"
          },
          {
            "expr": "rate(minio_list_ops_total[1m])",
            "legendFormat": "LIST Operations",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Operations/sec"
          },
          {
            "format": "short"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "avg": true
        },
        "tooltip": {
          "shared": true
        }
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "type": "graph",
        "title": "Cache Performance",
        "targets": [
          {
            "expr": "rate(minio_cache_hits_total[1m])",
            "legendFormat": "Cache Hits/sec",
            "refId": "A"
          },
          {
            "expr": "rate(minio_cache_misses_total[1m])",
            "legendFormat": "Cache Misses/sec",
            "refId": "B"
          },
          {
            "expr": "(rate(minio_cache_hits_total[1m]) / (rate(minio_cache_hits_total[1m]) + rate(minio_cache_misses_total[1m]))) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Rate"
          },
          {
            "format": "percent"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "avg": true
        }
      },
      {
        "id": 3,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "type": "graph",
        "title": "API Latency Percentiles",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(minio_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(minio_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P90 Latency",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(minio_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, rate(minio_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 Latency",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "alert": {
          "name": "High P99 Latency",
          "conditions": [
            {
              "evaluator": {
                "params": [50],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["D", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "message": "P99 latency exceeded 50ms threshold",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 4,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "type": "graph",
        "title": "Replication Performance",
        "targets": [
          {
            "expr": "minio_replication_lag_seconds",
            "legendFormat": "Replication Lag (seconds)",
            "refId": "A"
          },
          {
            "expr": "rate(minio_replication_throughput_total[1m])",
            "legendFormat": "Replication Throughput (ops/sec)",
            "refId": "B"
          },
          {
            "expr": "rate(minio_replication_errors_total[1m])",
            "legendFormat": "Replication Errors/sec",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Value"
          }
        ],
        "alert": {
          "name": "High Replication Lag",
          "conditions": [
            {
              "evaluator": {
                "params": [100],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "message": "Replication lag exceeded 100ms threshold",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 5,
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "type": "stat",
        "title": "Cache Write Throughput",
        "targets": [
          {
            "expr": "rate(minio_put_ops_total[1m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 100000
                },
                {
                  "color": "green",
                  "value": 500000
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "type": "stat",
        "title": "Cache Read Throughput",
        "targets": [
          {
            "expr": "rate(minio_get_ops_total[1m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 500000
                },
                {
                  "color": "green",
                  "value": 2000000
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "type": "stat",
        "title": "Average Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(minio_request_duration_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 16
        },
        "type": "stat",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "(rate(minio_cache_hits_total[1m]) / (rate(minio_cache_hits_total[1m]) + rate(minio_cache_misses_total[1m]))) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 22
        },
        "type": "graph",
        "title": "Storage Metrics",
        "targets": [
          {
            "expr": "minio_bytes_stored_total",
            "legendFormat": "Total Bytes Stored",
            "refId": "A"
          },
          {
            "expr": "minio_object_count_total",
            "legendFormat": "Total Object Count",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Storage"
          },
          {
            "format": "short",
            "label": "Objects"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=\"High P99 Latency\",alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Performance Alerts",
          "tagKeys": "alertname,severity"
        }
      ]
    }
  }
}
